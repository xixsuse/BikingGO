<?xml version="1.0" encoding="utf-8"?>
<!--
        This file defines default values of settings. The values will be
        used when the needed value is not yet defined by the user
-->
<resources>

    <!-- the directory contains data for engine -->
    <string name="data_dir">/sdcard/BikingData/</string>

    <!-- 是否顯示氣象資訊 -->
    <bool name="meteorol">false</bool>

    <!-- 是否開啟連線提示 -->
    <bool name="inetConfirmEnabled">true</bool>

    <!-- 是否於導航開始前詢問使用者是否進行軌跡錄製 -->
    <bool name="trackConfirmEnabled">true</bool>

    <!-- 是否開啟使用聲明提示 -->
    <bool name="isAnnounceNecessary">true</bool>

    <!-- POI查詢的距離範圍(In meter) -->
    <integer name="surroundRange">1000</integer>

    <!-- 是否開啟POI -->
    <bool name="poiEnabled">false</bool>

    <!-- 是否開啟GCN -->
    <bool name="gcmEnabled">true</bool>

    <!-- 是否開啟GPS -->
    <bool name="gpsEnabled">true</bool>

    <!-- 路徑規畫方式(PathFinder.BICYCLE) -->
    <integer name="routingMethod">1</integer>

    <!-- 地圖顯示方式(MapView.2D) -->
    <integer name="mapStyle">2</integer>
    
    <!-- 地圖STYLE -->
    <integer name="mapViewType">0</integer>

    <!-- 語音類型(eeego.SOUND_CHINESE) -->
    <integer name="soundType">1</integer>

    <!-- 使用者年齡(0 for unknown) -->
    <integer name="userAge">25</integer>

    <!-- 使用者性別(0 for unknown) -->
    <integer name="userSex">1</integer>

    <!-- 使用者身高(0 for unknown) -->
    <integer name="userHeight">170</integer>

    <!-- 使用者體重(0 for unknown) -->
    <integer name="userWeight">60</integer>

    <!-- 使用者體重(0 for unknown) -->
    <integer name="mapDownloadArea">1</integer>
</resources>